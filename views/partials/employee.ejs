<% if (visible) { %>
    <tr>
        <td><img alt="<%= name %>'s photo" src="/images/<%- imageUrl %>"></td>
        <td><%= name %></td>
        <td><%= age %></td>
        <td><%= occupation %></td>
        <td><%= company %></td>
    </tr>
<% } else { %>
    <tr>
        <td colspan="99"><%= name %> has hidden their information.</td>
    </tr>
<% } %>